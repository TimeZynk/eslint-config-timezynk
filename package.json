{
    "name": "eslint-config-timezynk",
    "version": "1.0.4",
    "description": "The eslint configuration used in TimeZynk JavaScript and TypeScript projects.",
    "homepage": "https://github.com/TimeZynk/eslint-config-timezynk",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "npx prettier --write index.js react rules typeScript package.json",
        "version": "npm run format && git add -A index.js react rules typeScript package.json",
        "postversion": "git push && git push --tags"
    },
    "keywords": [
        "eslint",
        "eslintconfig"
    ],
    "author": "Timezynk",
    "license": "MIT",
    "peerDependencies": {
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "babel-eslint": ">=8.2.0",
        "eslint": ">=7.7",
        "eslint-plugin-prettier": "3.1.4",
        "eslint-plugin-import": ">= 0.1",
        "eslint-plugin-jsx-a11y": "^6.4.1"
    },
    "devDependencies": {
        "prettier": "^2.2.1"
    }
}
